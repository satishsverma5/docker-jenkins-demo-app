

node("master"){
  stage'build'
  withDockerContiner("kohsuke/acmecorp-buildeny"){
  git "https://github.com/uaarkoti/docker-jenkins-demo-app.git"
  sh 'mvn install'
  archive 'target/*.war'
 }
    docker.withRegistry("https://index.docker.io/v1/','e470a1ab-1302-402c-9187-90b7f4f55669'){
    stage'package'
    docker.build('lavaliere/acmecorp-app').push('demo')
   
	
	stage 'deploy'
	sh './deploy.sh
    }
}

